  "preferGlobal": true,
  "bin": {
    "appsapp": "cli/appsapp.js"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/miegli/appsapp.cli"
  },
  "author": {
    "name": "Michael Egli",
    "email": "michael.egli@appsapp.io"
  },
  "keywords": [
    "angular",
    "ionic"
  ],
