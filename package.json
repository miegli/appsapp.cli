{
  "name": "appsapp-cli",
  "version": "1.6.30",
  "license": "MIT",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n appsapp-module -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "playground:build": "tsc -p playground -w",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\"",
    "test": "tsc",
    "p": "git add . && git commit -m 'new release' && git push && npm i && npm run build && npm publish dist",
    "preversion": "npm test",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags && npm publish dist"
  },
  "devDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.10",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "angular-in-memory-web-api": "^0.3.2",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.15.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "node-sass": "^4.5.2",
    "node-sass-tilde-importer": "^1.0.0",
    "node-watch": "^0.5.2",
    "protractor": "~5.1.0",
    "rollup": "^0.49.3",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.1.0",
    "systemjs": "^0.20.12",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "zone.js": "^0.8.4",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "clear": "0.0.1",
    "clui": "^0.3.6",
    "commander": "^2.11.0",
    "figlet": "^1.2.0",
    "firebase-admin": "^5.5.0",
    "github": "^12.0.1",
    "glob": "^7.1.2",
    "inquirer": "^4.0.0",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "preferences": "^1.0.0",
    "replace-in-file": "^3.0.0",
    "sha1": "^1.1.1",
    "simple-git": "^1.81.1",
    "touch": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.2.3",
    "rxjs": "^5.5.2",
    "base-64": "^0.1.0",
    "node-cmd": "^3.0.0",
    "node-watch": "^0.5.5",
    "angular2-uuid": "^1.1.1",
    "angularfire2": "=5.0.0-rc.0",
